{
  "bin": {
    "zenny": "bin/zenny.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cli-color": "^2.0.0",
    "elliptic": "^6.5.1",
    "es6-error": "^4.1.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "pug": "^2.0.4",
    "ramda": "^0.26.1",
    "statuses": "^1.5.0",
    "superagent": "^5.1.1",
    "swagger-ui-express": "^4.1.2",
    "threads": "^1.0.0-beta.8",
    "timeago.js": "^4.0.1",
    "yargs": "^15.0.2"
  },
  "description": "Zenny, a cryptocurrency demonstration",
  "devDependencies": {
    "@inc/eslint-config": "^2019.10.22",
    "chronver": "^2019.10.2-7.1",
    "coveralls": "^3.0.8",
    "eslint": "^6.6.0",
    "husky": "^3.1.0",
    "mermaid.cli": "^0.5.1",
    "mocha": "^6.2.2",
    "mocha-steps": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0",
    "supertest": "^4.0.2",
    "updates": "^9.1.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/conradoqg/zenny",
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run increment && git add -A :/"
    }
  },
  "license": "Apache-2.0",
  "name": "zenny",
  "repository": {
    "type": "git",
    "url": "https://github.com/conradoqg/zenny.git"
  },
  "scripts": {
    "compose:up": "docker-compose up",
    "coverage": "nyc --reporter=lcov --reporter=text --clean node node_modules/mocha/bin/_mocha -u bdd --colors test/",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "diagrams": "mmdc -i doc/blockchain.mmd -o doc/blockchain.png -b transparent && mmdc -i doc/components.mmd -o doc/components.png -b transparent",
    "format": "eslint . --fix",
    "increment": "chronver --increment package",
    "start": "node bin/zenny.js",
    "--test": "_mocha -u bdd --colors test/",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:lint": "standardx --verbose | snazzy"
  },
  "version": "0.0.2"
}
